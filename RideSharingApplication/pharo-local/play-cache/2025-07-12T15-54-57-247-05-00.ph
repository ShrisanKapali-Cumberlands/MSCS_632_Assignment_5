"Create the system"system := RideSharingSystem new."Create drivers"driver1 := Driver new initializeWithID: 1 name: 'Mark Joel' rating: 4.8.driver2 := Driver new initializeWithID: 2 name: 'Tom Henry' rating: 4.9."Create riders"rider1 := Rider new initializeWithID: 1 name: 'Henny Patrick'.rider2 := Rider new initializeWithID: 2 name: 'Onil Lakers'."Create different types of rides"standardRide1 := StandardRide new initializeWithID: 1 pickup: 'Downtown' dropoff: 'Airport' distance: 15.premiumRide1 := PremiumRide new initializeWithID: 2 pickup: 'Hotel' dropoff: 'Conference Center' distance: 8.standardRide2 := StandardRide new initializeWithID: 3 pickup: 'Mall' dropoff: 'University' distance: 12.premiumRide2 := PremiumRide new initializeWithID: 4 pickup: 'Restaurant' dropoff: 'Theater' distance: 5."Assign rides to drivers"driver1 addRide: standardRide1.driver1 addRide: premiumRide1.driver2 addRide: standardRide2.driver2 addRide: premiumRide2."Add rides to riders"rider1 requestRide: standardRide1.rider1 requestRide: premiumRide2.rider2 requestRide: premiumRide1.rider2 requestRide: standardRide2."Add to system"system addDriver: driver1.system addDriver: driver2.system addRider: rider1.system addRider: rider2.system addRide: standardRide1.system addRide: premiumRide1.system addRide: standardRide2.system addRide: premiumRide2."Display results"Transcript clear.Transcript show: driver1 getDriverInfo; cr; cr.Transcript show: driver2 getDriverInfo; cr; cr.Transcript show: rider1 viewRides; cr; cr.Transcript show: system demonstratePolymorphism; cr.